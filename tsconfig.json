{
  "compilerOptions": {
    "moduleResolution": "node",
    "types": ["@nuxt/types", "@nuxt/schema", "@supabase/supabase-js"],
    "paths": {
      "@/*": ["./"]
    },
    "strict": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true
  },
  "include": ["./**/*.ts", "./**/*.d.ts", "./**/*.vue"],
  "extends": "./.nuxt/tsconfig.json"
}
